{
    "name": "cyndaron/cyndaron",
    "description": "Simple and lightweight CMS",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Cyndaron\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Cyndaron/module-geelhoed"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Cyndaron/module-minecraft"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/view": "^6.5",
        "conedevelopment/blade-filters": "^0.7",
        "ext-pdo": "*",
        "ext-imagick": "*",
        "ext-json": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "components/jquery": "^3.5",
        "ckeditor/ckeditor": "dev-full/4.7.x",
        "twbs/bootstrap": "^4.5",
        "symfony/http-foundation": "^5.0",
        "thecodingmachine/safe": "^1.1",
        "cyndaron/module-geelhoed": "dev-master",
        "cyndaron/module-minecraft": "dev-master",
        "symfony/mailer": "^5.2"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phan/phan": "^3.0",
        "thecodingmachine/phpstan-safe-rule": "^1.0",
        "phpstan/phpstan": "^0.12.42",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "scripts": {
        "phpstan": [
            "vendor/bin/phpstan analyse src --level 8"
        ],
        "cscheck": [
            "vendor/bin/php-cs-fixer fix src/ --allow-risky=yes --dry-run --diff"
        ]
    }
}
